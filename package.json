{
  "name": "menu-management-app",
  "version": "25.0.1",
  "description": "Skeleton app for a DHIS2 web app",
  "main": "src/index.html",
  "scripts": {
    "coverage": "babel-node node_modules/.bin/isparta cover --root ./src --report text --report html --report lcov node_modules/mocha/bin/_mocha -- --reporter dot --require config/setup.js --recursive test",
    "prebuild": "rm -rf build && mkdir build && d2-manifest package.json manifest.webapp",
    "build": "webpack",
    "postbuild": "cp -r index.html icon.png node_modules/babel-polyfill/dist/polyfill.min.js manifest.webapp src/i18n build/",
    "test": "mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test",
    "test-watch": "mocha --reporter spec --compilers js:babel-core/register --require config/setup.js --recursive test --watch",
    "lint": "eslint --fix src && ./node_modules/eslint/bin/eslint.js --env node,mocha --global expect,sinon,spy,stub --rule 'no-unused-expressions: 0' test && scss-lint",
    "validate": "npm ls --depth 0",
    "prestart": "d2-manifest package.json manifest.webapp",
    "start": "webpack-dev-server"
  },
  "keywords": [
    "DHIS2",
    "App"
  ],
  "author": "Mark Polak",
  "license": "BSD-3-Clause",
  "dependencies": {
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "recompose": "^0.20.2"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "d2": "^25.0.11",
    "d2-manifest": "^1.0.0",
    "d2-ui": "^25.4.7",
    "d2-utilizr": "^0.2.9",
    "enzyme": "^2.1.0",
    "eslint": "^2.4.0",
    "eslint-config-dhis2": "^2.0.2",
    "eslint-plugin-react": "^4.1.0",
    "isparta": "^4.0.0",
    "jsdom": "^9.0.0",
    "loglevel": "^1.4.0",
    "material-ui": "^0.15.4",
    "mocha": "^2.4.5",
    "node-sass": "^3.4.2",
    "react": "^15.3.2",
    "react-addons-test-utils": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-tap-event-plugin": "^1.0.0",
    "sass-loader": "^3.2.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "style-loader": "^0.13.0",
    "webpack": "^2.1.0-beta.25",
    "webpack-dev-server": "^2.1.0-beta.8"
  },
  "pre-commit": [
    "test",
    "lint",
    "validate"
  ],
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dhis2/menu-management-app.git"
  },
  "bugs": {
    "url": "https://github.com/dhis2/menu-management-app/issues"
  },
  "homepage": "https://github.com/dhis2/menu-management-app#readme",
  "manifest.webapp": {
    "icons": {
      "48": "icon.png"
    },
    "developer": {
      "url": "",
      "name": "Mark Polak"
    },
    "activities": {
      "dhis": {
        "href": ".."
      }
    }
  }
}
